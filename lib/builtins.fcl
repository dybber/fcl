sig while : ([a] -> bool) -> ([a] -> <a>lvl) -> <a>lvl -> [a]
fun while cond step x = #while cond step x

sig whileSeq : (a -> bool) -> (a -> a) -> a -> a
fun whileSeq cond step x = #whileSeq cond step x

